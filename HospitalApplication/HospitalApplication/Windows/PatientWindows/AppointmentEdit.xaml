<Window x:Class="HospitalApplication.Windows.Patient1.WindowExaminationEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalApplication.Windows.Patient1"
        mc:Ignorable="d"
        Title="WindowExaminationEdit" Height="800" Width="500"
        Style="{StaticResource WindowStyle}">
    
    <Grid Margin="10">
        <StackPanel>
            <Label Content="Choose new doctor for examination."/>
            <ComboBox Margin="0 10" Name="Combo" SelectedIndex="0">
            </ComboBox>
            <Button Content="OK" x:Name="ButtonOk" Click="ButtonOk_Click"/>
            <Label Content="" Name="oldDoctor"></Label>
            <Label Content="" Name="newDoctor"></Label>
        </StackPanel>
    </Grid>
</Window>
